<h1>hello <%= data %>
</h1>

<table class="table">
    <thead>
        <tr>
            <th>Sn</th>
            <th>Name</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        <% list.forEach((item,index)=>{%>
            <tr>
                <td>
                    <%= index+1 %>
                </td>
                <td>
                    <%= item.name %>
                </td>
                <td>
                    <%= item.address %>
                </td>
            </tr>
            <% })%>
    </tbody>
</table>